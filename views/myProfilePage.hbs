<div class="profileDiv">
    <h1>{{user.username}}</h1>
    <div class="userImage">
        <img src="/images/{{user.profilePic}}" />
    </div>
    <p><span>Age:</span> {{user.age}}</p>
    <p><span>Location:</span> {{user.location}}</p>
    <p><span>Bio:</span> {{user.bio}}</p>
    <a href="/myProfile/edit">Edit Profile</a>
</div>
<div class="clipGridDiv">
    <h1>My Clips</h1>
    {{#each clips}}
        {{> myClipDiv clip=this}}
    {{/each}}
</div>